{
  "version": 1,
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "defaultProject": "fit-yummy-bar",
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "strict": false
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "projects": {
    "fit-yummy-bar": {
      "projectType": "application",
      "root": "apps/fit-yummy-bar",
      "sourceRoot": "apps/fit-yummy-bar/src",
      "prefix": "fit-yummy",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/fit-yummy-bar",
            "index": "apps/fit-yummy-bar/src/index.html",
            "main": "apps/fit-yummy-bar/src/main.ts",
            "polyfills": "apps/fit-yummy-bar/src/polyfills.ts",
            "tsConfig": "apps/fit-yummy-bar/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/fit-yummy-bar/src/favicon.ico",
              "apps/fit-yummy-bar/src/assets"
            ],
            "styles": ["apps/fit-yummy-bar/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/fit-yummy-bar/src/environments/environment.ts",
                  "with": "apps/fit-yummy-bar/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "fit-yummy-bar:build:production"
            },
            "development": {
              "browserTarget": "fit-yummy-bar:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "fit-yummy-bar:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/fit-yummy-bar/src/**/*.ts",
              "apps/fit-yummy-bar/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/fit-yummy-bar"],
          "options": {
            "jestConfig": "apps/fit-yummy-bar/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "fit-yummy-bar-e2e": {
      "root": "apps/fit-yummy-bar-e2e",
      "sourceRoot": "apps/fit-yummy-bar-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/fit-yummy-bar-e2e/cypress.json",
            "tsConfig": "apps/fit-yummy-bar-e2e/tsconfig.e2e.json",
            "devServerTarget": "fit-yummy-bar:serve:development"
          },
          "configurations": {
            "production": {
              "devServerTarget": "fit-yummy-bar:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/fit-yummy-bar-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "ui": {
      "root": "libs/ui",
      "sourceRoot": "libs/ui/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/ui/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/ui"],
          "options": {
            "jestConfig": "libs/ui/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "products-e2e": {
      "root": "apps/products-e2e",
      "sourceRoot": "apps/products-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/products-e2e/cypress.json",
            "tsConfig": "apps/products-e2e/tsconfig.e2e.json",
            "devServerTarget": "products:serve:development"
          },
          "configurations": {
            "production": {
              "devServerTarget": "products:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/products-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "fit-yummy-admin": {
      "projectType": "application",
      "root": "apps/fit-yummy-admin",
      "sourceRoot": "apps/fit-yummy-admin/src",
      "prefix": "fit-yummy",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/fit-yummy-admin",
            "index": "apps/fit-yummy-admin/src/index.html",
            "main": "apps/fit-yummy-admin/src/main.ts",
            "polyfills": "apps/fit-yummy-admin/src/polyfills.ts",
            "tsConfig": "apps/fit-yummy-admin/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/fit-yummy-admin/src/favicon.ico",
              "apps/fit-yummy-admin/src/assets"
            ],
            "styles": ["apps/fit-yummy-admin/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/fit-yummy-admin/src/environments/environment.ts",
                  "with": "apps/fit-yummy-admin/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "fit-yummy-admin:build:production"
            },
            "development": {
              "browserTarget": "fit-yummy-admin:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "fit-yummy-admin:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/fit-yummy-admin/src/**/*.ts",
              "apps/fit-yummy-admin/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/fit-yummy-admin"],
          "options": {
            "jestConfig": "apps/fit-yummy-admin/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "fit-yummy-admin-e2e": {
      "root": "apps/fit-yummy-admin-e2e",
      "sourceRoot": "apps/fit-yummy-admin-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/fit-yummy-admin-e2e/cypress.json",
            "tsConfig": "apps/fit-yummy-admin-e2e/tsconfig.e2e.json",
            "devServerTarget": "fit-yummy-admin:serve:development"
          },
          "configurations": {
            "production": {
              "devServerTarget": "fit-yummy-admin:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/fit-yummy-admin-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "products": {
      "projectType": "library",
      "root": "libs/products",
      "sourceRoot": "libs/products/src",
      "prefix": "fit-yummy",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/products"],
          "options": {
            "jestConfig": "libs/products/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/products/src/**/*.ts",
              "libs/products/src/**/*.html"
            ]
          }
        }
      }
    },
    "users": {
      "projectType": "library",
      "root": "libs/users",
      "sourceRoot": "libs/users/src",
      "prefix": "users",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:ng-packagr-lite",
          "options": {
            "project": "libs/users/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/users/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "libs/users/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/users"],
          "options": {
            "jestConfig": "libs/users/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/users/src/**/*.ts",
              "libs/users/src/**/*.html"
            ]
          }
        }
      }
    },
    "orders": {
      "projectType": "library",
      "root": "libs/orders",
      "sourceRoot": "libs/orders/src",
      "prefix": "orders",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:ng-packagr-lite",
          "options": {
            "project": "libs/orders/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/orders/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "libs/orders/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/orders"],
          "options": {
            "jestConfig": "libs/orders/jest.config.js",
